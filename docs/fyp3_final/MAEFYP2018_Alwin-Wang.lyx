\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:BVF"
plural "false"
caps "false"
noprefix "false"

\end_inset

.
\begin_inset Formula 
\begin{equation}
\nu\frac{\partial\omega}{\partial n}\equiv\sigma\label{eq:BVF}
